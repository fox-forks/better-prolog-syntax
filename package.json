{
    "name": "better-prolog-syntax",
    "displayName": "Better Prolog Syntax",
    "description": "",
    "version": "0.1.3",
    "icon": "icon.png",
    "scripts": {},
    "repository": {
        "url": "https://github.com/jeff-hykin/better-prolog-syntax.git"
    },
    "keywords": [
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "author": {
        "name": "Jeff Hykin"
    },
    "publisher": "jeff-hykin",
    "contributes": {
        "languages": [
            {
                "id": "prolog",
                "aliases": [
                    "Prolog",
                    "prolog"
                ],
                "extensions": [
                    ".pl"
                ],
                "configuration": "./prolog.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "prolog",
                "scopeName": "source.prolog",
                "path": "./autogenerated/prolog.tmLanguage.json",
                "embeddedLanguages": {}
            },
            {
                "language": "perl",
                "scopeName": "source.prolog",
                "path": "./autogenerated/prolog.tmLanguage.json",
                "embeddedLanguages": {}
            }
        ]
    },
    "devDependencies": {
        "chalk": "^2.4.2",
        "dictionary-en-us": "^2.1.1",
        "lodash": "^4.17.19",
        "nspell": "^2.1.2",
        "oniguruma": "^7.2.0",
        "textmate-bailout": "^1.1.0",
        "vsce": "^1.99.0",
        "textmate-tester": "^1.1.0"
    }
}